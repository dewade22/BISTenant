
@{
    ViewData["Title"] = "Labeling";
    Layout = "~/Views/Shared/Material/_Layouts.cshtml";
}

<section id="content">
    <div class="container">
        <div class="c-header">
            <h2 class="text-capitalize">@ViewData["Title"] - @ViewBag.BomName</h2>
        </div>
        <div class="card">
            <div class="card-header">
                <h2 class="card-title">@ViewData["Title"] Data</h2>
            </div>
            <div class="card-body card-padding">
                <h1 class="text-capitalize">Coming Soon !!</h1>
            </div>
        </div>
    </div>
</section>